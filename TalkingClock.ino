void setup(){
  pinMode(inPin, INPUT);
  Serial.begin(9600);  // Setup RTC via I2C protocol
  Wire.begin();
  RTC.begin();
  if (RTC.isrunning()) { 
    Serial.print("1");
  } 
  else {
    RTC.adjust(DateTime(__DATE__, __TIME__));
    Serial.print("2");
  }
  now = RTC.now();
  LcdInitialise();
  LcdClear();
}

void loop() {
  now = RTC.now(); //get current time from the Real Time Clock module.
  hr=now.hour();
  m=now.minute();
  s=now.second();
  d=now.day();
  //wd=now.weekday();
  mn=now.month();
  y=now.year();
  
  if(hr==0) {
    h=hr+12;
    am=1;
    pm=0;
    morn=1;
    aftr=0;
    even=0;
  }    
  else if(hr<=12) {
    h=hr;
    am=1;
    pm=0;
    morn=1;
    aftr=0;
    even=0;
  } 
  else if (hr<17) {
    h=hr%12;
    am=0;
    pm=1;
    morn=0;
    aftr=1;
    even=0;
  }
  else {
    h=hr%12;
    am=0;
    pm=1;
    morn=0;
    aftr=0;
    even=1;
  }
  ShowTime();
  
  val = digitalRead(inPin);
  if (val == HIGH) {
    SpeakTime();
    Serial.print("HIGH val = ");
    Serial.print(val);
    Serial.println();
  }
  else if(val == LOW) {
  Serial.print("LOW val = ");
    Serial.print(val);
    Serial.println();
  }
}


void ShowTime() {
  Serial.println();
  Serial.print(h);
  Serial.print(":");
  Serial.print(m);
  Serial.print(":");
  Serial.print(s);
  if(am==1) {
    Serial.print("AM");
  } else {
    Serial.print("PM");
  }
  Serial.println();
  Serial.print(d);
  Serial.print("/");
  Serial.print(mn);
  Serial.print("/");
  Serial.print(y);
  Serial.println();
  //Serial.print(wd);
  //Serial.println();
  
  gotoXY(0,5);
  displayTime(h);
  LcdString(":");
  displayTime(m);
  LcdString(":");
  displayTime(s);
  if (am == 1) {
    LcdString(" AM");
  }
  else if (pm == 1) {
    LcdString(" PM");
  }
  gotoXY(2,5);
  displayTime(d);
  LcdString("/");
  displayTime(mn);
  LcdString("/");
  yr=y%2000;
  LcdString("20");
  displayTime(yr);
  //gotoXY(4,12);
  //LcdString("SATURDAY");
  delay(100);
}

void SpeakTime() {
   voice.say(spPAUSE1);
   if (morn == 1) {
      voice.say(spGOOD);
      voice.say(spMORNING);
    }
    else if (aftr == 1) {
      voice.say(spGOOD);
      voice.say(spAFTERNOON);
    }
    else if (even == 1) {
      voice.say(spGOOD);
      voice.say(spEVENING);
    }
    voice.say(spPAUSE1);
    voice.say(spTHE);
    voice.say(spTIME);
    voice.say(spIS);
    speak(h);
    speak(m);
    if (am == 1) {
      voice.say(spA_M_);
    }
    else if (pm == 1) {
      voice.say(spP_M_);
    }
    voice.say(spPAUSE1);
  }
